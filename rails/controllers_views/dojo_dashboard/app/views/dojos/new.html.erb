<div class="container">
    <div class="row">
        <h1>New Dojo</h1>
    </div>
    <div class="row">
        <form action='<%= dojos_path %>' method='POST'>
            <input type='hidden' name='authenticity_token' value='<%= form_authenticity_token %>'>
            <div class="col">
                <p><label for="dojo[branch]">Branch: </label></p>
                <p><label for="dojo[street]">Street: </label></p>
                <p><label for="dojo[city]">City: </label></p>
                <p><label for="dojo[state]">State: </label></p>
            </div>
            <div class="col">
                <p><input type="text" name='dojo[branch]'></p>
                <p><input type="text" name='dojo[street]'></p>
                <p><input type="text" name='dojo[city]'></p>
                <p><input type="text" name='dojo[state]'></p>
                <button type='submit'>Create</button>
            </div>
        </form>
    </div>
    <div class="row">
        <%= link_to "Go Back", dojos_path %>
    </div>
</div>